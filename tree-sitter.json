{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "akyno",
      "camelcase": "Akyno",
      "title": "Akyno",
      "scope": "source.akyno",
      "path": ".",
      "file-types": [
        "akn"
      ],
      "injection-regex": "^akyno$",
      "class-name": "TreeSitterAkyno",
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Akyno grammar for tree-sitter",
    "authors": [
      {
        "name": "Anokhi Suresh",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/anoaky/tree-sitter-akyno",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
